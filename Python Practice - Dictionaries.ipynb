    "#david Beck september 10, 2021\n",
    "li=list(map(str,input().split()))\n",
    "\n",
    "d={}\n",
    "\n",
    "for item in li:\n",
    "\n",
    "    l=len(item)\n",
    "    if l in d.keys():\n",
    "        tlist=d[l]\n",
    "        tlist.append(item)\n",
    "        d[l]=tlist\n",
    "    else:\n",
    "        temp=[]\n",
    "        temp.append(item)\n",
    "        d[l]=temp\n",
    "print(d)"
   ]
