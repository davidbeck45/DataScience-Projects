    "# David Beck 09/5/2021\n",
    "n = int(input(\"Enter number: \"))\n",
    "count = 0\n",
    "while n != 0:\n",
    "    x = n%10\n",
    "    if x ==5:\n",
    "        count += 1\n",
    "    n = n // 10\n",
    "print(\"5 appears in the given number \",count, \" times.\")"
